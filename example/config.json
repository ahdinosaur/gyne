{
  "docker": {
    "host": "/var/run/docker.sock"
  },
  "networks": [
    {
      "name": "test_up_network"
    }
  ],
  "volumes": [
    {
      "name": "test_up_volume"
    }
  ],
  "services": [
    {
      "name": "test_up_service",
      "taskTemplate": {
        "containerSpec": {
          "image": "nginx:alpine"
        }
      }
    }
  ],
  "stacks": [
    {
      "name": "test_up_stack",
      "stack": {
        "version": "3",
        "services": {
          "viz": {
            "image": "dockersamples/visualizer",
            "volumes": [
              "/var/run/docker.sock:/var/run/docker.sock"
            ],
            "ports": [
              "8080:8080"
            ]
          }
        }
      }
    }
  ]
}

