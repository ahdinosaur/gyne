{
  "name": "docker-up",
  "version": "0.0.0",
  "description": "manage a swarm of Docker stacks, networks, and volumes",
  "main": "index.js",
  "bin": "bin.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "node-dev example",
    "style:lint": "standard",
    "style:format": "prettier-standard '**/*.js' '**/*.md'",
    "style": "npm-run-all -s style:format style:lint",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier-standard",
        "standard",
        "git add"
      ],
      "*.md": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "standard": {
    "globals": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/docker-up.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/docker-up/issues"
  },
  "homepage": "https://github.com/ahdinosaur/docker-up#readme",
  "devDependencies": {
    "dependency-check": "^2.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "prettier-standard": "^8.0.1",
    "run-default": "^1.0.0",
    "standard": "^11.0.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "docker-remote-api": "^5.0.0",
    "es2040": "^1.2.3",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.10",
    "run-parallel": "^1.1.9",
    "run-series": "^1.1.8",
    "run-waterfall": "^1.1.6",
    "simple-get": "^3.0.2",
    "tempy": "^0.2.1"
  }
}
